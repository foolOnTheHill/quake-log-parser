{
  "name": "quake-log-parser",
  "version": "1.0.0",
  "description": "Parses log files from Quake games",
  "main": "main.js",
  "dependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "minimist": "^1.2.0",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "bundle": "browserify src/main.js -o dist/quake-log-parser.js -t [ babelify --presets [ es2015 ] ]",
    "build": "babel src --presets babel-preset-es2015 --out-dir build",
    "watch": "watchify src/main.js -o dist/quake-log-parser.js -t [ babelify --presets [ es2015 ] ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foolOnTheHill/quake-log-parser.git"
  },
  "keywords": [
    "quake",
    "log",
    "parser"
  ],
  "author": "George Oliveira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foolOnTheHill/quake-log-parser/issues"
  },
  "homepage": "https://github.com/foolOnTheHill/quake-log-parser#readme"
}
